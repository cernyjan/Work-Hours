{
    "description": "Easy Punch in and Punch out",
    "manifest_version": 2,
    "name": "Work Hours",
    "version": "1.1",
    "homepage_url": "https://github.com/cernyjan/Work-Hours",
    "icons": {
        "48": "icons/clock-48.png"
    },
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["jquery-3.2.1.min.js"]
        }
    ],
    "permissions": [
        "tabs", "http://walrus.gmc.net/workhours/*"
    ],
    "browser_action": {
        "default_icon": {
        "16": "icons/clock-48.png",
        "32": "icons/clock-48.png"
        },
        "default_popup": "popup.html"
    }
}